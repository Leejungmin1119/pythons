    "#https://www.acmicpc.net/problem/24313\n",
    "#알고리즘 수업-점근적 표기1(실버 5)\n",
    "\n",
    "\"\"\"\n",
    "조건\n",
    "1.f(n) = a1n + a0 ,  f(n) ≤ c * g(n)\n",
    "\n",
    "2. g(n) = n\n",
    "\n",
    "3. a1n ≤ c\n",
    "\n",
    "\"\"\"\n",
    "\n",
    "a_1,a_0 = map(int,input().split())\n",
    "\n",
    "C = int(input())\n",
    "\n",
    "n= int(input())\n",
    "\n",
    "if (a_1*n + a_0)<= (C*n) and a_1 <= C:\n",
    "    print(1)\n",
    "else:\n",
    "    print(0)    \n",
    "\n",
    "\n"
   ]
  }
