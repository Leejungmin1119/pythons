   "source": [
    "import sys\n",
    "input=sys.stdin.readline\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "sum_ = 0\n",
    "for i in range(1, n+1): \n",
    "\t# i의 배수의 개수 = i를 약수로 갖는 수\n",
    "    sum_ += (n//i)*i  \n",
    "\n",
    "print(sum_)\n",
    "#2+2"
   ]
  }
